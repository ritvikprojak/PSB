					<p name="folderpath" type="string" qi="Path to folder to contain a copy of each ingested file" v="@APPVAR(values/gen/ProcessedFolder)"/>
				</a>
				<a name="scan" ns="com:Datacap.Libraries.mvScan.Actions" qi=""/>
			</func>
		</rule>
